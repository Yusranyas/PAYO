<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6AAED0">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="400dp"
        android:layout_height="580dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="91dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="60dp"
        android:src="@drawable/backgroundd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="137dp"
        android:layout_marginBottom="541dp"
        android:fontFamily="@font/inter_semibold"
        android:text="Login"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="221dp"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="491dp"
        android:text="Better Today. Easy Online Payment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="199dp"
        android:layout_marginEnd="103dp"
        android:layout_marginBottom="513dp"
        android:text="Make Your Experience Payment More"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/signup2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="142dp"
        android:layout_marginTop="512dp"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="205dp"
        android:text="Dont Have Account? Sign Up"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="71dp"
        android:layout_marginTop="405dp"
        android:layout_marginEnd="213dp"
        android:layout_marginBottom="312dp"
        android:text="Forgot Password? Click Here"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/et_name1"
        android:layout_width="269dp"
        android:layout_height="50dp"
        android:layout_marginLeft="32sp"
        android:layout_marginTop="340dp"
        android:layout_marginRight="32sp"
        android:layout_marginBottom="341dp"
        android:background="@drawable/email"
        android:fontFamily="@font/poppins_thin"
        android:hint="Enter your Password"
        android:paddingLeft="16sp"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_name2"
        android:layout_width="265dp"
        android:layout_height="50dp"
        android:layout_marginLeft="32sp"
        android:layout_marginTop="272dp"
        android:layout_marginRight="32sp"
        android:layout_marginBottom="409dp"
        android:background="@drawable/email"
        android:fontFamily="@font/poppins_thin"
        android:hint="Enter your email"
        android:paddingLeft="16sp"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/login"
        android:layout_width="260dp"
        android:layout_height="49dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="449dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="233dp"
        android:backgroundTint="#1A497A"
        android:text="Login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="260dp"
        android:layout_height="49dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="596dp"
        android:layout_marginEnd="79dp"
        android:layout_marginBottom="86dp"
        android:backgroundTint="#1A497A"
        android:text="or continue with...."
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="260dp"
        android:layout_height="49dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="547dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="135dp"
        android:backgroundTint="#1A497A"
        android:text="or continue with Google"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>