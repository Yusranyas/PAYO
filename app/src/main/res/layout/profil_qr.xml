<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#6AAED0">

    <ImageView
        android:id="@+id/kembali"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="52dp"
        android:src="@drawable/panah"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/foto"/>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="24dp"
        android:src="@drawable/code"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/info_saldo"
        tools:layout_editor_absoluteX="331dp" />

    <ImageView
        android:id="@+id/foto"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/tri"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/kembali"
        tools:layout_editor_absoluteY="92dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/nama"
        android:layout_width="120dp"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/background"
        android:fontFamily="@font/poppin"
        android:hint="Tri Mulia Bahar"
        android:textColorHint="#000"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@id/tlpn"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@id/foto"
        app:layout_constraintTop_toBottomOf="@id/kembali" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/tlpn"
        android:layout_width="120dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/background"
        android:fontFamily="@font/poppin"
        android:hint="08123*****77"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@id/info_saldo"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@id/foto"
        app:layout_constraintTop_toBottomOf="@id/nama" />

    <ImageView
        android:id="@+id/info_saldo"
        android:layout_width="350dp"
        android:layout_height="110dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="35dp"
        android:src="@drawable/history1"
        app:layout_constraintBottom_toTopOf="@id/help"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/foto" />

    <ImageView
        android:id="@+id/payo"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="3dp"
        android:src="@drawable/payo"
        app:layout_constraintEnd_toStartOf="@id/saldo_uang"
        app:layout_constraintStart_toStartOf="@id/info_saldo"
        app:layout_constraintTop_toTopOf="@id/info_saldo"
        app:layout_constraintBottom_toTopOf="@id/unduh"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/saldo"
        android:layout_width="25dp"
        android:layout_height="13dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Saldo"
        android:textColorHint="#000"
        android:textSize="9sp"
        app:layout_constraintLeft_toRightOf="@id/payo"
        app:layout_constraintTop_toTopOf="@id/info_saldo"
        app:layout_constraintBottom_toTopOf="@id/saldo_uang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/saldo_uang"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Rp110.000.000"
        android:textColorHint="#000"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@id/payo"
        app:layout_constraintTop_toBottomOf="@id/saldo" />

    <ImageView
        android:id="@+id/unduh"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:src="@drawable/unduh"
        app:layout_constraintBottom_toBottomOf="@id/info_saldo"
        app:layout_constraintStart_toStartOf="@id/info_saldo"
        app:layout_constraintTop_toBottomOf="@id/payo"
        app:layout_constraintEnd_toStartOf="@id/masuk_uang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/uang_masuk"
        android:layout_width="60dp"
        android:layout_height="13dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Uang Masuk"
        android:textColorHint="#000"
        android:textSize="9sp"
        app:layout_constraintLeft_toRightOf="@id/unduh"
        app:layout_constraintTop_toBottomOf="@id/saldo_uang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/masuk_uang"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="+Rp110.000"
        android:textColorHint="#000"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@id/info_saldo"
        app:layout_constraintStart_toEndOf="@id/unduh"
        app:layout_constraintTop_toBottomOf="@id/uang_masuk"
        app:layout_constraintVertical_bias="0.09" />

    <ImageView
        android:id="@+id/upload"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="58dp"
        android:layout_marginStart="70dp"
        android:src="@drawable/upload"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="247dp"
        app:layout_constraintBottom_toBottomOf="@id/info_saldo"
        app:layout_constraintTop_toTopOf="@id/info_saldo"
        app:layout_constraintStart_toEndOf="@id/masuk_uang"
        app:layout_constraintEnd_toStartOf="@id/uang_keluar"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/uang_keluar"
        android:layout_width="60dp"
        android:layout_height="13dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Uang Keluar"
        android:textColorHint="#000"
        android:textSize="9sp"
        app:layout_constraintLeft_toRightOf="@id/upload"
        app:layout_constraintTop_toBottomOf="@id/saldo_uang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/keluar_uang"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="-Rp110.000"
        android:textColorHint="#000"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@id/info_saldo"
        app:layout_constraintStart_toEndOf="@id/upload"
        app:layout_constraintTop_toBottomOf="@id/uang_keluar"
        app:layout_constraintVertical_bias="0.09" />

    <ImageView
        android:id="@+id/help"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/transaksi"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="308dp"
        app:layout_constraintTop_toBottomOf="@id/info_saldo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/help_center"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Help Center"
        android:textColorHint="#000"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@id/help"
        app:layout_constraintTop_toTopOf="@id/help" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/help_kata"
        android:layout_width="300dp"
        android:layout_height="10dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Kami siap melayani kendala yang sedang anda alami, silahkan konsultasikan dengan agen kami"
        android:textColorHint="#000"
        android:textSize="7sp"
        app:layout_constraintStart_toStartOf="@id/help"
        app:layout_constraintTop_toBottomOf="@id/help_center" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/tentang_kata"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/background"
        android:fontFamily="@font/poppin"
        android:hint="Tentang Akun"
        android:textColorHint="#000"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/help"
        app:layout_constraintBottom_toTopOf="@id/tentang"
        app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
        android:id="@+id/tentang"
        android:layout_width="350dp"
        android:layout_height="140dp"
        android:src="@drawable/transaksi"
        android:layout_marginTop="3dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="379dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tentang_kata"/>



    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/tentang1"
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Pengaturan Profile"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/tentang"
        app:layout_constraintTop_toTopOf="@id/tentang"
        app:layout_constraintBottom_toTopOf="@id/tentang2"
        app:layout_constraintRight_toRightOf="@id/tentang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/tentang2"
        android:layout_width="130dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Pengaturan Keamanan"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/tentang"
        app:layout_constraintTop_toBottomOf="@id/tentang1"
        app:layout_constraintBottom_toTopOf="@id/tentang3"
        app:layout_constraintRight_toRightOf="@id/tentang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/tentang3"
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Akun Terhubung"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/tentang"
        app:layout_constraintTop_toBottomOf="@id/tentang2"
        app:layout_constraintBottom_toTopOf="@id/tentang4"
        app:layout_constraintRight_toRightOf="@id/tentang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/tentang4"
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Verifikasi"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/tentang"
        app:layout_constraintTop_toBottomOf="@id/tentang3"
        app:layout_constraintBottom_toTopOf="@id/tentang5"
        app:layout_constraintRight_toRightOf="@id/tentang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/tentang5"
        android:layout_width="140dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Pengaturan Pembayaran"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/tentang"
        app:layout_constraintTop_toBottomOf="@id/tentang4"
        app:layout_constraintRight_toRightOf="@id/tentang"
        app:layout_constraintBottom_toBottomOf="@id/tentang"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/lainnya_kata"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/background"
        android:fontFamily="@font/poppin"
        android:hint="Lainnya"
        android:textColorHint="#000"
        android:textSize="14sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tentang"
        app:layout_constraintBottom_toTopOf="@id/lainnya"/>

    <ImageView
        android:id="@+id/lainnya"
        android:layout_width="350dp"
        android:layout_height="57.5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:tooltipText="3dp"
        android:src="@drawable/transaksi"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="551dp"
        app:layout_constraintTop_toBottomOf="@id/lainnya_kata"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/lainnya1"
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Syarat &amp; Ketentuan"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintRight_toRightOf="@id/tentang"
        app:layout_constraintStart_toStartOf="@id/lainnya"
        app:layout_constraintTop_toTopOf="@id/lainnya"
        app:layout_constraintBottom_toTopOf="@id/lainnya2"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/lainnya2"
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Kebijakan Privasi"
        android:textColorHint="#000"
        android:textSize="12sp"
        app:layout_constraintRight_toRightOf="@id/tentang"
        app:layout_constraintStart_toStartOf="@id/lainnya"
        app:layout_constraintTop_toBottomOf="@id/lainnya1" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/ganti"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_marginTop="24dp"
        android:layout_marginStart="31dp"
        android:background="@drawable/history1"
        android:fontFamily="@font/poppin"
        android:hint="Ganti Akun"
        android:paddingLeft="42dp"
        android:textColorHint="#000"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lainnya" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/keluar"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/out"
        android:fontFamily="@font/poppin"
        android:hint="Keluar"
        android:paddingLeft="60dp"
        android:textColorHint="#000"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/ganti"
        app:layout_constraintTop_toBottomOf="@id/lainnya" />

    <ImageView
        android:id="@+id/info_qr"
        android:layout_width="350dp"
        android:layout_height="540dp"
        android:src="@drawable/qris"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>